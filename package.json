{
  "name": "visual-tree",
  "displayName": "visual-tree",
  "description": "VisualTree extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visual-tree.showUI",
        "title": "VisualTree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "cd webview && vite build",
    "watch": "cd webview && vite build --watch",
    "dev": "cd webview && vite"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}