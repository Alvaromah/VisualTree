  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.2.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "dompurify": "^3.2.2",
    "eslint": "^9.13.0",
    "markdown-it": "^14.1.0",
    "tailwindcss": "^3.4.16"
  }
}