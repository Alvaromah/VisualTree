{
  "name": "visual-tree",
  "displayName": "visual-tree",
  "description": "VisualTree extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visual-tree.showUI",
        "title": "VisualTree"
      }
